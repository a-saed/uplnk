{
  "name": "@uplnk/core",
  "version": "0.1.0",
  "description": "Framework-agnostic file uploads with progress for signed URLs and HTTP endpoints",
  "keywords": [
    "upload",
    "file-upload",
    "progress",
    "xhr",
    "signed-url",
    "s3",
    "gcp",
    "azure",
    "batch-upload",
    "retry",
    "validation"
  ],
  "author": "Abdulrhman Elsaed",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/a-saed/uplnk.git",
    "directory": "packages/core"
  },
  "homepage": "https://a-saed.github.io/uplnk/",
  "bugs": {
    "url": "https://github.com/a-saed/uplnk/issues"
  },
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts",
    "clean": "rm -rf dist",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@uplnk/types": "^0.1.0"
  },
  "peerDependencies": {},
  "devDependencies": {
    "@types/node": "^20.0.0",
    "jsdom": "^25.0.1",
    "tsup": "^8.3.5",
    "typescript": "^5.6.3",
    "vitest": "^2.1.6"
  }
}
