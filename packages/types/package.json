{
  "name": "@uplnk/types",
  "version": "0.1.0",
  "description": "Shared TypeScript types for uplnk",
  "keywords": [
    "upload",
    "types",
    "typescript",
    "file-upload"
  ],
  "author": "Abdulrhman Elsaed",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/a-saed/uplnk.git",
    "directory": "packages/types"
  },
  "homepage": "https://a-saed.github.io/uplnk/",
  "bugs": {
    "url": "https://github.com/a-saed/uplnk/issues"
  },
  "type": "module",
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts",
    "clean": "rm -rf dist",
    "test": "echo 'No tests in @uplnk/types'"
  },
  "devDependencies": {
    "tsup": "^8.3.5",
    "typescript": "^5.6.3"
  }
}
